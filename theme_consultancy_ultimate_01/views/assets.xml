<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="snippet_ultimate_footer_option" inherit_id="website.snippet_options">
        <t t-call="web_editor.snippet_options"/>
        <xpath expr="//we-button[@data-customize-website-views='website.footer_custom']"
               position="before">
            <we-button title="Consultancy Theme Footer"
                       data-customize-website-views="theme_consultancy_ultimate_01.consultancy_theme_footer"
                       data-customize-website-variable="'consultancy-theme-footer'"
                       data-img="">consultancy Theme Footer
            </we-button>
        </xpath>
    </template>
</odoo>