<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Secuencia de Contra-Recibos para Facturas de Clientes --> 
        <record id="seq_invoice_counter_receipt_customer" model="ir.sequence">
            <field name="name">Contra-Recibos de Clientes</field>
            <field name="code">account.invoice.counter.receipt.customer</field>
            <field name="prefix">CC.</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Secuencia de Contra-Recibos para Facturas de Proveedores --> 
        <record id="seq_invoice_counter_receipt_supplier" model="ir.sequence">
            <field name="name">Contra-Recibos de Proveedores</field>
            <field name="code">account.invoice.counter.receipt.supplier</field>
            <field name="prefix">CR.</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
    </data>
</odoo>