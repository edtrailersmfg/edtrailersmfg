               position="after">
            <we-select string="Ultimate Button Style" data-variable="ultimate-button" data-reload="/">
                <we-button title="None"
                           data-name="button_option_none"
                           data-customize-website-variable="'button-default'"
                           data-customize-website-views=""
                           data-img="">None
                </we-button>
                <we-button title="Button 1"
                           data-name="button_option_1"
                           data-customize-website-variable="'button-1'"
                           data-customize-website-views="website_customize_theme_business_ul_73lines.option_button_1"
                           data-img="/website_customize_theme_business_ul_73lines/static/src/imgs/button_icons/button-style-1.png">
                </we-button>
                <we-button title="Button 2"
                           data-name="button_option_2"
                           data-customize-website-variable="'button-2'"
                           data-customize-website-views="website_customize_theme_business_ul_73lines.option_button_2"
                           data-img="/website_customize_theme_business_ul_73lines/static/src/imgs/button_icons/button-style-2.png">
