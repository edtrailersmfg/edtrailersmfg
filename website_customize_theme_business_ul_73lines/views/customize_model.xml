            <we-button data-name="option_header_navbar_hover_bottom_border"
                       data-customize-website-variable="'option-nav-hover-bottom-border'"
                       data-customize-website-views="website_customize_theme_business_ul_73lines.option_nav_hover_bottom_border">
                Hover
                Bottom Border
            </we-button>
        </xpath>

        <!-- Page Layout -->
        <xpath expr="//we-select[@data-variable='layout']"
               position="inside">
            <we-button data-customize-website-variable="'fluid'">Fluid
            </we-button>
        </xpath>

        <!-- Transparent Navbar -->
