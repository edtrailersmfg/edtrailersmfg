                </we-button>
                <we-button title="Button 5"
                           data-name="button_option_5"
                           data-customize-website-variable="'button-5'"
                           data-customize-website-views="website_customize_theme_business_ul_73lines.option_button_5"
                           data-img="/website_customize_theme_business_ul_73lines/static/src/imgs/button_icons/button-style-5.png">
                </we-button>
                <we-button title="Button 6"
                           data-name="button_option_6"
                           data-customize-website-variable="'button-6'"
                           data-customize-website-views="website_customize_theme_business_ul_73lines.option_button_6"
                           data-img="/website_customize_theme_business_ul_73lines/static/src/imgs/button_icons/button-style-6.png">
                </we-button>
                <we-button title="Button 7"
                           data-name="button_option_7"
                           data-customize-website-variable="'button-7'"
                           data-customize-website-views="website_customize_theme_business_ul_73lines.option_button_7"
