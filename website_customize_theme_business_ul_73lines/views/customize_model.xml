                           data-customize-website-variable="'nav-transparent-font-primary'"
                           data-customize-website-views="website_customize_theme_business_ul_73lines.option_nav_transparent_font_primary"
                           data-img="/website_customize_theme_business_ul_73lines/static/src/imgs/header-icon/header-icon-1.svg">
                </we-button>
            </we-select>
        </xpath>

        <!-- Input Field -->
        <xpath expr="//we-input[@data-variable='input-border-width']"
               position="before">
            <we-select string="Input Fields" data-variable="input-field" data-reload="/">
                <we-button title="Input Field Default"
                           data-name="input_field_default"
                           data-customize-website-views=""
                           data-customize-website-variable="'input-default'"
                           data-img="/website_customize_theme_business_ul_73lines/static/src/imgs/input-icon/input-icon-1.png">
                </we-button>
                <we-button title="Input Field 1"
                           data-name="option_input_1"
                           data-customize-website-variable="'input-1'"
                           data-customize-website-views="website_customize_theme_business_ul_73lines.option_input_1"
