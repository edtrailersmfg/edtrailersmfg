        </xpath>

        <!-- Midheader Style -->
        <xpath expr="//we-select[@data-variable='layout']"
               position="before">
            <we-select string="Mid Header" data-variable="mid-header" data-reload="/">
                <we-button title="Top Mid Header None"
                           data-name="midheader_default"
                           data-customize-website-variable="'mid-header-default'"
                           data-customize-website-views="">Mid Header None
                </we-button>
                <we-button title="Top Mid Header 1"
                           data-name="midheader_1"
                           data-customize-website-variable="'mid-header-1'"
                           data-customize-website-views="website_customize_theme_business_ul_73lines.option_mid_header_1"
                           data-img="/website_customize_theme_business_ul_73lines/static/src/imgs/header-icon/header-icon-7.svg">
                </we-button>
                <we-button title="Top Mid Header 2"
                           data-name="midheader_2"
                           data-customize-website-variable="'mid-header-2'"
                           data-customize-website-views="website_customize_theme_business_ul_73lines.option_mid_header_6"
