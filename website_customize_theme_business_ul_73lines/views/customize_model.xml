               position="after">
            <we-select string="Transparent Navbar" data-variable="transparent-navbar" data-reload="/">
                <we-button title="Nav Transparent + Font Default"
                           data-name="option_nav_transparent_font_default"
                           data-customize-website-variable="'nav-transparent-font-default'"
                           data-customize-website-views=""
                           data-img="/website_customize_theme_business_ul_73lines/static/src/imgs/header-icon/header-icon-1.svg">
                </we-button>
                <we-button title="Nav Transparent + Font White"
                           data-name="option_nav_transparent_font_white"
                           data-customize-website-variable="'nav-transparent-font-white'"
                           data-customize-website-views="website_customize_theme_business_ul_73lines.option_nav_transparent_font_white"
                           data-img="/website_customize_theme_business_ul_73lines/static/src/imgs/header-icon/header-icon-1.svg">
                </we-button>
                <we-button title="Nav Transparent + Font Black"
                           data-name="option_nav_transparent_font_black"
