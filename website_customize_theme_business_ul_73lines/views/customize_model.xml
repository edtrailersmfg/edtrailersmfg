            <we-button data-name="option_header_navbar_pills_round"
                       data-customize-website-variable="'option-nav-pills-round'"
                       data-customize-website-views="website_customize_theme_business_ul_73lines.option_nav_pils_round">
                Pills + Round
            </we-button>
            <we-button data-name="option_header_navbar_color_primary_on_active"
                       data-customize-website-variable="'option-nav-color-primary'"
                       data-customize-website-views="website_customize_theme_business_ul_73lines.option_nav_color_primary">
                Color Primary On
