        <!-- Ultimate Language Flag -->
        <xpath expr="//we-button[@data-customize-website-views='website.header_language_selector_flag']"
               position="before">
            <we-button
                    data-customize-website-views="website.header_language_selector_flag, website_customize_theme_business_ul_73lines.square_flag, website_customize_theme_business_ul_73lines.square_flag_img"
                    data-customize-website-variable="'square-flag'">Square Flag
            </we-button>
        </xpath>
    </template>

    <template id="square_flag" inherit_id="portal.language_selector" active="False">
        <xpath expr="//span[hasclass('align-middle')]" position="attributes">
            <attribute name="class" add="d-none" separator=" "/>
        </xpath>
    </template>

    <template id="square_flag_img" inherit_id="website.lang_flag" active="False">
        <xpath expr="//img[hasclass('o_lang_flag')]" position="replace">
            <img class="o_lang_flag" t-attf-src="#{flag_image_src}?height=25" style="width: 25px;height: 20px;border-radius: 0px;"/>
        </xpath>
    </template>
</odoo>
