    <template id="s_object_carousel_style_option"
              inherit_id="website.snippet_options">
        <xpath expr="//div[@data-js='Box']" position="after">
            <div data-js=''
                 data-selector=".mini_product_carousel">
                <we-select string="Alignment" data-no-preview="true">
                    <we-button data-select-class="">None</we-button>
                    <we-button data-select-class="text-left">
                        Left
                    </we-button>
                    <we-button data-select-class="text-center">
                        Center
                    </we-button>
                    <we-button data-select-class="text-right">
                        Right
                    </we-button>
                </we-select>

                <we-select string="Box Shadow" data-no-preview="true">
                    <we-button data-select-class="">
                        None
                    </we-button>
                    <we-button data-select-class="box_shadow_mini">
                        Box Shadow
                    </we-button>
                </we-select>
            </div>
        </xpath>
    </template>

    <!--Hide Sign In/Register Button From Ecommerce Mid Header Snippets-->
    <template id="ecommerce_mid_header_hide_sign_in_ultimate"
              inherit_id="portal.frontend_layout" customize_show="True"
              name="Hide Sign In From Dropdown" active="False">
        <xpath expr="//div[@id='wrapwrap']" position="attributes">
            <attribute name="t-attf-class"
                       add="#{'o_sign_in_hide_ultimate_ecommerce'}"
                       separator=" "/>
        </xpath>
    </template>
    <template id="ecommerce_mid_header_hide_register_ultimate"
