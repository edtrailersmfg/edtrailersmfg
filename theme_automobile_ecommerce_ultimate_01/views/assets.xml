<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="snippet_ultimate_footer_option" inherit_id="website.snippet_options">
        <t t-call="web_editor.snippet_options"/>
        <xpath expr="//we-button[@data-customize-website-views='website.footer_custom']"
               position="before">
            <we-button title="Automobile Ecommerce Footer"
                       data-customize-website-views="theme_automobile_ecommerce_ultimate_01.automobile_ecommerce_footer"
                       data-customize-website-variable="'automobile-ecommerce-footer'"
                       data-img="">Automobile Ecommerce Footer
            </we-button>
        </xpath>
    </template>
</odoo>