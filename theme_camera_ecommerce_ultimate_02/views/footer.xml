                                        <div class="media-body">
                                            <span t-field="res_company.street" class="text-600"/>
                                            <span t-field="res_company.city" class="text-600"/>
                                            <span t-field="res_company.state_id.name" class="text-600"/>
                                            <span t-field="res_company.zip" class="text-600"/>
                                            <span t-field="res_company.country_id.name" class="text-600"/>
