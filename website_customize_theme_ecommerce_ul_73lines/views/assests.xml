    <template id="ul_full_search" inherit_id="portal.frontend_layout"
              customize_show="True"
              name="Full Screen Search in Navbar" active="False">
        <xpath expr="//header" position="attributes">
            <attribute name="t-attf-class" add="#{'o_show_full_search'}"
                       separator=" "/>
        </xpath>
    </template>
