    <!--Hide Add To Cart From Navbar Option-->
    <template id="cart_hide_nav" inherit_id="portal.frontend_layout"
              customize_show="True"
              name="Hide Add To Cart  Navbar" active="False">
        <xpath expr="//header" position="attributes">
            <attribute name="t-attf-class" add="#{'o_add_to_cart_nav_hide'}"
                       separator=" "/>
        </xpath>
    </template>
    <template id="business_layout_inherit" inherit_id="portal.frontend_layout"
              customize_show="True"
              name="Hide Wishlist Navbar" active="False">
        <xpath expr="//header" position="attributes">
            <attribute name="t-attf-class" add="#{'o_wishlist_nav_hide'}"
                       separator=" "/>
        </xpath>
    </template>

