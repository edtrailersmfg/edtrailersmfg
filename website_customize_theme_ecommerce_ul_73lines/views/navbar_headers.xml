            inherit_id="website_customize_theme_ecommerce_ul_73lines.mid_header_bottom_right"
            active="True"
            name="Mid Header Bottom Wish List" customize_show="True">
        <xpath expr="//span[hasclass('mid-bottom-wish')]" position="replace">
            <t t-call="website_sale_wishlist.header_wishlist_link">
                <t t-set="_icon" t-value="True"/>
                <t t-set="_item_class" t-value="'nav-item list-inline-item'"/>
                <t t-set="_link_class" t-value="'nav-link'"/>
            </t>
        </xpath>
    </template>

    <template id="header_searchbox" name="Header SearchBox">
        <div class="search-hide">
            <t t-snippet-call="website_ecommerce_snippets_blocks_ul_73lines.s_searchbar_input_ultimate"/>
        </div>
    </template>

    <template id="header_searchbox_with_dropdown" name="Header SearchBox With Dropdown">
        <div class="search-dropdown-hide">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown" style="padding:10px;">
                <span class="fa fa-search fa-2x" style="font-size: 18px;"/>
            </a>
            <ul class="dropdown-menu py-0 px-3 border-0 bg-transparent" role="menu">
                <div>
                    <li class="navbar-form form-group m-0">
                        <t t-snippet-call="website_ecommerce_snippets_blocks_ul_73lines.s_searchbar_input_ultimate"/>
                    </li>
                </div>
            </ul>
        </div>
    </template>

