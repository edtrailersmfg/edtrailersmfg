    <template id="template_navbar_15" inherit_id="website.layout" name="Template Navbar 15" active="False">
        <xpath expr="//header//nav" position="replace">
            <nav data-name="Navbar"
                 class="ultimate_nav navbar navbar-expand-md navbar-light o_colored_level o_cc shadow-sm">
                <div id="top_menu_container" class="container justify-content-start justify-content-lg-between">
                    <div class="row">
                        <!-- Brand -->
                        <t t-call="website.placeholder_header_brand">
                            <t t-set="_link_class" t-value="'mr-4'"/>
                        </t>
                        <!-- Navbar Toggler -->
                        <t t-call="website.navbar_toggler">
                            <t t-set="_toggler_class" t-value="'ml-auto'"/>
                        </t>

                        <!-- Navbar Collapse -->
                        <div id="top_menu_collapse" class="collapse navbar-collapse col-lg-7">
                            <t t-call="website.navbar_nav">
                                <t t-set="_nav_class" t-value="'ml-auto'"/>
                                <!-- Menu -->
                                <t t-foreach="website.menu_id.child_id" t-as="submenu">
                                    <t t-call="website.submenu">
                                        <t t-set="item_class" t-value="'nav-item'"/>
                                        <t t-set="link_class" t-value="'nav-link'"/>
                                    </t>
                                </t>
                                <li class="nav-item ul_brand_nav">
                                    <a role="menuitem" href="/brand-list" class="nav-link">
                                        <span>All Brands
                                        </span>
                                    </a>
                                </li>

                                <!-- Add to Cart -->
                                <t t-call="website_sale.header_cart_link">
                                    <t t-set="_icon" t-value="True"/>
                                    <t t-set="_item_class" t-value="'nav-item'"/>
                                    <t t-set="_link_class" t-value="'nav-link'"/>
                                </t>
                                <!-- Wishlist -->
                                <t t-call="website_sale_wishlist.header_wishlist_link">
                                    <t t-set="_icon" t-value="True"/>
                                    <t t-set="_item_class" t-value="'nav-item'"/>
                                    <t t-set="_link_class" t-value="'nav-link'"/>
                                </t>
                                <!-- Sign In -->
                                <t t-call="portal.placeholder_user_sign_in">
                                    <t t-set="_item_class" t-value="'nav-item'"/>
                                    <t t-set="_link_class" t-value="'nav-link font-weight-bold'"/>
                                </t>
                                <!-- User Dropdown -->
                                <t t-call="portal.user_dropdown">
                                    <t t-set="_user_name" t-value="true"/>
                                    <t t-set="_item_class" t-value="'nav-item dropdown'"/>
                                    <t t-set="_link_class" t-value="'nav-link font-weight-bold'"/>
                                </t>
                                <!-- Language Selector -->
                                <li>
                                    <t t-call="website.placeholder_header_language_selector">
                                        <t t-set="_div_classes" t-value="'ml-2'"/>
                                    </t>
                                </li>
                            </t>
                        </div>
                        <t t-call="website_customize_theme_ecommerce_ul_73lines.header_searchbox"></t>
                        <t t-call="website_customize_theme_ecommerce_ul_73lines.header_searchbox_with_dropdown"></t>
                    </div>
                </div>
            </nav>
        </xpath>

        <xpath expr="//div[@id='top_menu_collapse']" position="after">
            <t t-call="website_ecommerce_snippets_blocks_ul_73lines.full_screen_search_d1"></t>
        </xpath>

    </template>

    <template id="template_navbar_16" inherit_id="website.layout" name="Template Navbar 16" active="False">
        <xpath expr="//header//nav" position="replace">
