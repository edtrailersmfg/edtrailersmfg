            inherit_id="website_customize_theme_ecommerce_ul_73lines.mid_header_bottom_right"
            active="True"
            name="Mid Header Bottom Add to Cart" customize_show="True">
        <xpath expr="//span[hasclass('mid-bottom-add-to-cart')]"
               position="replace">
            <t t-call="website_sale.header_cart_link">
                <t t-set="_icon" t-value="True"/>
                <t t-set="_item_class" t-value="'nav-item list-inline-item'"/>
                <t t-set="_link_class" t-value="'nav-link'"/>
            </t>
        </xpath>
    </template>
    <template
